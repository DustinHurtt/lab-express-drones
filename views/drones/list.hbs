<h2>Available drones</h2>
<div class="">
<div class=" ">
    {{#each drones}}

    <ul>

  
        <p> {{this.name}} </p>
        <p> {{this.propellers}} </p>
        <p> {{this.maxSpeed}} </p>

        <a href="/drones/{{this._id}}/edit">Update Drone</a>

        <form method = "post" action="/drones/{{this._id}}/delete">
            <input type="submit" value="Delete Drone" />
        </form>


</ul>


    {{/each}}

</div>







